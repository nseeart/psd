{
    "name": "@n.see/psd-parser",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "main": "./dist/psd-parser.cjs.js",
    "module": "./dist/psd-parser.esm.js",
    "exports": {
        ".": {
            "import": "./dist/psd-parser.esm.js",
            "require": "./dist/psd-parser.cjs.js"
        }
    },
    "types": "types/main.d.ts",
    "files": [
        "dist",
        "types"
    ],
    "scripts": {
        "dev": "tsc && cross-env NODE_ENV=development rollup --watch --config rollup.config.ts --configPlugin typescript",
        "build": "tsc && cross-env NODE_ENV=production rollup --config rollup.config.ts --configPlugin typescript"
    },
    "dependencies": {
        "buffer": "^6.0.3",
        "iconv-lite": "~0.4.24",
        "lodash-es": "^4.17.21",
        "pngjs": "3.2.0",
        "rsvp": "~3.0.21",
